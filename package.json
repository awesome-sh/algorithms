{
  "name": "algorithms",
  "version": "0.0.1",
  "description": "",
  "main": "jest.config.js",
  "scripts": {
    "test": "jest --coverage && tsc && standard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "standard": {
    "env": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": false,
    "coverageDirectory": "<rootDir>/.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lquixada/algorithms.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lquixada/algorithms/issues"
  },
  "homepage": "https://github.com/lquixada/algorithms#readme",
  "lint-staged": {
    "*.js": "standard --fix"
  },
  "dependencies": {
    "@types/node": "14.6.2",
    "string-hash": "1.1.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.10",
    "husky": "4.2.5",
    "jest": "26.4.2",
    "lint-staged": "10.2.13",
    "standard": "14.3.4",
    "ts-jest": "26.3.0",
    "typescript": "4.0.2"
  }
}
