{
  "name": "algorithms",
  "version": "0.0.1",
  "description": "",
  "main": "jest.config.js",
  "scripts": {
    "all": "jest --coverage && tsc && eslint . --ext .ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "typecheck": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 96,
        "functions": 100,
        "lines": 99,
        "statements": 99
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lquixada/algorithms.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lquixada/algorithms/issues"
  },
  "homepage": "https://github.com/lquixada/algorithms#readme",
  "dependencies": {
    "@types/node": "14.14.37",
    "string-hash": "1.1.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.23.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.2.5",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "typescript": "4.2.4"
  }
}